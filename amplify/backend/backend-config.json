{
	"api": {
		"CloudComputingAPI": {
			"service": "API Gateway",
			"providerPlugin": "awscloudformation",
			"dependsOn": [
				{
					"category": "function",
					"resourceName": "CloudComputingFunction",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	},
	"storage": {
		"CloudComputingDB": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"CloudComputingFunction": {
			"service": "Lambda",
			"providerPlugin": "awscloudformation",
			"build": true,
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "CloudComputingDB",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}